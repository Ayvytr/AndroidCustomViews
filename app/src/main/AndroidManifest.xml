<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.ayvytr.androidcustomviews"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".testactivity.QuickIndexViewActivity"/>
        <activity android:name=".testactivity.SuperEditTextActivity"/>
        <activity android:name=".testactivity.numberpickerview.NumberPickerActivity"/>
        <activity android:name=".testactivity.numberpickerview.ActivityTimePicker"/>
        <activity android:name=".testactivity.ClearableEditTextActivity">
        </activity>
        <activity android:name=".testactivity.PasswordEditTextActivity">
        </activity>
    </application>

</manifest>